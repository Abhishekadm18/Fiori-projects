<mvc:View  controllerName="project18.controller.Product"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">

    <Page id="_IDGenPage1" title="{i18n>detailPageTitles}" showNavButton="true" navButtonPress=".onNavBack" >
      <content>
        <Table id="_IDGenTable1" items="{ProductModel>/}">
          <columns>
            
              <Column id="_IDGenColumn2">
                <Text id="_IDGenText3" text="OrderID"/>
              </Column>
                <Column id="_IDGenColumn3">
                <Text id="_IDGenText4" text="ProductID"/>
              </Column>
              <Column id="_IDGenColumn4">
                <Text id="_IDGenText5" text="UnitPrice"/>
              </Column>
              <Column id="_IDGenColumn5">
                <Text id="_IDGenText6" text="Quantity"/>
              </Column>
              <Column id="_IDGenColumn6">
                <Text id="_IDGenText7" text="Discount"/>
              </Column>
          </columns>
          <items>
            <ColumnListItem id="_IDGenColumnListItem1" type="Navigation" >
              <cells>
                <Text id="_IDGenText21"  text="{ProductModel>OrderID}"/>
                <Text id="_IDGenText31"  text="{ProductModel>ProductID}"/>
                <Text id="_IDGenText41"  text="{ProductModel>UnitPrice}"/>
                <Text id="_IDGenText51"  text="{ProductModel>Quantity}"/>
                <Text id="_IDGenText61"  text="{ProductModel>Discount}"/>
              </cells>
            </ColumnListItem>
          </items>
        </Table>
      </content>
    </Page>

</mvc:View> 